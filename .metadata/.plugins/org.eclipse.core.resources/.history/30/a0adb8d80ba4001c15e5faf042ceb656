
public class Control2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		boolean android = false ;
		boolean iOS = true;
		//만약에 android 가 true 라면
		if (android == true)
		{
			System.out.println("안드로이드 입니다.");
		}
		else
		//그렇지 않다면
		{
			System.out.println("iOS입니다.");
			//iOS입니다. 출력
		}
	}

}
